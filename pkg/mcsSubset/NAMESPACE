

import("stats", "graphics")


useDynLib("mcsSubset")


## export new generics
##
export("mcsSubset",
       "refit")


## 'mcsSubset' generator methods
##
S3method("mcsSubset", "formula")
S3method("mcsSubset", "lm")
S3method("mcsSubset", "default")


## 'mcsSubset' extractor methods
##
S3method("print", "mcsSubset")
S3method("plot", "mcsSubset")
S3method("variable.names", "mcsSubset")
S3method("formula", "mcsSubset")
S3method("model.frame", "mcsSubset")
S3method("model.matrix", "mcsSubset")
S3method("refit", "mcsSubset")
S3method("coef", "mcsSubset")
S3method("vcov", "mcsSubset")
S3method("fitted", "mcsSubset")
S3method("residuals", "mcsSubset")
S3method("deviance", "mcsSubset")
S3method("logLik", "mcsSubset")
S3method("AIC", "mcsSubset")

S3method("summary", "mcsSubset")
S3method("print", "summary.mcsSubset")
S3method("plot", "summary.mcsSubset")
